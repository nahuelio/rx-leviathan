{
  "name": "@nahuelio/rx-leviathan-compiler",
  "version": "1.0.0",
  "description": "RxLeviathan Compiler",
  "keywords": [
    "leviathan",
    "compiler"
  ],
  "author": "Patricio Ferreira <3dimentionar@gmail.com>",
  "homepage": "https://github.com/nahuelio/rx-leviathan/tree/master/packages/rx-leviathan-compiler#readme",
  "license": "UNLICENSED",
  "main": "dist/rxlc.js",
  "module": "dist/es/rxlc.js",
  "types": "dist/rxlc.d.ts",
  "bin": {
    "rxlc": "./bin/rxlc"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist/**/*.js",
    "dist/*.d.ts",
    "dist/bin/rxlc",
    "dist/bin/rxlc.cmd"
  ],
  "repository": "nahuelio/rx-leviathan-compiler",
  "scripts": {
    "build-dev": "nodemon --exec \"rollup -c --silent\"",
    "build": "cross-env ENV=prod rollup -c",
    "test": "cross-env ENV=test nodemon --exec \"mocha --w --config .mocharc.js \\\"test/**/*.test.ts\\\"\"",
    "coverage": "cross-env ENV=test nyc mocha --config .mocharc.js \\\"test/**/*.test.ts\\\""
  },
  "bugs": {
    "url": "https://github.com/nahuelio/rx-leviathan/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "exports": {
    ".": {
      "node": {
        "require": "./dist/rxlc.js",
        "import": "./dist/es/rxlc.js"
      },
      "default": "./dist/rxlc.js"
    }
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "**/*/*.d.ts"
    ],
    "temp-dir": ".cache",
    "report-dir": "coverage",
    "cache": true,
    "sourceMap": true,
    "all": true,
    "instrument": true
  },
  "dependencies": {
    "colorette": "1.2.1",
    "extend": "3.0.2",
    "json5": "2.1.3",
    "sander": "0.6.0"
  },
  "devDependencies": {
    "tslib": "2.0.1"
  }
}
