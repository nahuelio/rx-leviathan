{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"esmodules": true
				},
				"modules": "umd",
				"corejs": "3"
			}
		]
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime",
			{
				"absoluteRuntime": false,
				"corejs": "3",
				"helpers": true,
				"regenerator": true,
				"useESModules": true
			}
		]
	]
}
