{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": ["dom", "es5", "es6", "es7"],
		"moduleResolution": "node",
		"jsx": "react",
		"jsxFactory": "Leviathan.DOM.create",
		"inlineSourceMap": true,
		"baseUrl": "./",
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"experimentalDecorators": true
	},
	"include": [
		"./lib/js"
	],
	"exclude": [
		"node_modules"
	]
}
