{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"chrome": "58",
					"ie": "11",
					"firefox": "60"
				},
				"modules": "auto",
				"useBuiltIns": "entry",
				"corejs": 3
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties"
	],
	"exclude": "node_modules/**"
}
