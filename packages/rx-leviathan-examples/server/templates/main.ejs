<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>RxLeviathan - <%- pageName %></title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tachyons/4.11.1/tachyons.min.css" />
        <script src="https://cdn.jsdelivr.net/npm/systemjs/dist/system.min.js"></script>
    </head>
    <body>
        <div id="app"><%- app %></div>
        <script type="text/javascript">
            System.import('./libraries.js').then((RxLeviathan) => {
				System.import(('/<%- pageName %>.js'.toLowerCase())).then((module) => {
					RxLeviathan.default.render(module['<%- pageName %>Page'], document.getElementById('app'));
				});
			});
        </script>
    </body>
</html>
