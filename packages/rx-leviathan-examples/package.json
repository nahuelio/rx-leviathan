{
  "name": "@nahuelio/rx-leviathan-examples",
  "version": "1.0.0",
  "description": "leviathan examples",
  "keywords": [
    "leviathan",
    "compiler",
    "reactive",
    "examples"
  ],
  "author": "Patricio Ferreira <3dimentionar@gmail.com>",
  "homepage": "https://github.com/nahuelio/rx-leviathan/tree/master/packages/rx-leviathan-examples#readme",
